{
	"include": ["./src/**/*.ts", "./src/**/*.js"],
	"exclude": [],
	"compilerOptions": {
		"target": "ES2017",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "dist"
	}
}
